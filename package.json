{
  "name": "react-node-backend-app",
  "version": "1.0.0",
  "description": "[see site](https://www.freecodecamp.org/news/how-to-create-a-react-app-with-a-node-backend-the-complete-guide/)",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server:api\" \"cd client/ui-react && npm start\"",
    "clear": "rm -rf ./node_modules && rm package-lock.json && cd client/ui-react && rm -rf ./node_modules && rm package-lock.json",
    "postinstall": "cd client/ui-react && npm i",
    "server:api": "prism mock ./server/e-procurement.yaml -p 3001",
    "server:react": "npm run build && serve ./client/ui-react/build/ -p 4001",
    "build": "cd client/ui-react && npm run build",
    "prod": "concurrently \"npm run server:api\" \"npm run server:react\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stoplight/prism-cli": "^4.10.5",
    "concurrently": "^7.4.0",
    "serve": "^14.0.1"
  }
}
