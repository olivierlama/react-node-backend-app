{
  "name": "react-node-backend-app",
  "version": "1.0.0",
  "description": "[see site](https://www.freecodecamp.org/news/how-to-create-a-react-app-with-a-node-backend-the-complete-guide/)",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"cd client && npm start\"",
    "delete-package-lock": "rm package-lock.json && cd client && rm package-lock.json",
    "postinstall": "cd client && npm i",
    "server": "node server/index.js",
    "build": "cd client && npm run build",
    "prod": "npm i && npm run build && npm run server"
  },
